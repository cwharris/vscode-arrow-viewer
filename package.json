{
    "name": "apache-arrow-viewer",
    "displayName": "Arrow Inspector",
    "description": "Apache Arrow File Inspector",
    "version": "0.1.0",
    "publisher": "cwharris",
    "repository": "https://github.com/cwharris/vscode-arrow-viewer.git",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:arrowViewer.openInViewerFromActiveTextEditor",
        "onCommand:arrowViewer.openInViewerFromUri"
    ],
    "main": "./out/extension",
    "contributes": {
        "languages": [
            {
                "id": "arrow",
                "extensions": [
                    ".arrow"
                ]
            }
        ],
        "commands": [
            {
                "command": "arrowViewer.openInViewerFromActiveTextEditor",
                "title": "Arrow: Open in Viewer"
            },
            {
                "command": "arrowViewer.openInViewerFromUri",
                "title": "Open in Arrow Viewer"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "arrowViewer.openInViewerFromUri",
                    "when": "resourceExtname == .arrow",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/react": "16.4.16",
        "@types/react-dom": "16.0.9",
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "apache-arrow": "github:graphistry/arrow#348d735ef44f68199e7e6b66778a5bbda5f8d99c",
        "react": "16.5.2",
        "react-dom": "16.5.2",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21"
    }
}
